<view class="content">
<!-- 头像信息 -->
<view class="gwq_show_you">
  <view class="user_info_avatar">
    <open-data type="userAvatarUrl"></open-data>
  </view>
  <view class="user_info_name">
    <open-data type="userNickName"></open-data>
  </view>
</view>
<!-- 个人信息模块 -->
<view class="gwq_cells">
    <view class="gwq_cell_bd">
      <image src="/imgs/magnifier.png" class="icon"></image>
    </view>
    <view class="gwq_cell_bd">查看 / 修改个人信息</view>
    <view class="gwq_cell_ft"> > </view>
</view>
<!-- 二维码模块 -->
<view class="gwq_cells">
    <view class="gwq_cell_hd">
      <image src="/imgs/icon_paper_plane.png" class="icon"></image>
    </view>
    <view class="gwq_cell_bd">我的二维码</view>
    <view class="gwq_cell_ft"> > </view>
</view>
<!-- 租赁模块 -->
<view class="gwq_cells">
    <view class="gwq_cell_hd">
      <image src="/imgs/icon_in_process.png" class="icon"></image>
    </view>
    <view class="gwq_cell_bd">租赁进度</view>
     <view class="gwq_cell_ft"> > </view>
</view>
<view class="gwq_cells">
  <view class="gwq_cell_hd">
      <image src="/imgs/icon_time.png" class="icon"></image>
    </view>
    <view class="gwq_cell_bd">租赁历史</view>
     <view class="gwq_cell_ft"> > </view>
</view>
<view class="gwq_cells">
    <view class="gwq_cell_hd">
      <image src="/imgs/icon_deposit.png" class="icon"></image>
    </view>
    <view class="gwq_cell_bd">我的押金</view>
     <view class="gwq_cell_ft"> > </view>
</view>
<view class="gwq_cells">
    <view class="gwq_cell_hd">
      <image src="/imgs/icon_review_2.png" class="icon"></image>
    </view>
    <view class="gwq_cell_bd">我的评价</view>
     <view class="gwq_cell_ft"> > </view>
</view>
<!-- 意见反馈 -->
<view class="gwq_cells">
    <view class="gwq_cell_hd">
      <image src="/imgs/icon_help.png" class="icon"></image>
    </view>
    <view class="gwq_cell_bd">意见反馈</view>
     <view class="gwq_cell_ft"> > </view>
</view>
<view class="gwq_cells">
    <view class="gwq_cell_hd">
      <image src="/imgs/icon_info.png" class="icon"></image>
    </view>
    <view class="gwq_cell_bd">关于</view>
     <view class="gwq_cell_ft"> > </view>
</view>
<!-- 退出模块 -->
<view class="gwq_cells" bindtap="onLogout">
  <view class="gwq_cell gwq_cell_access" hover-class="gwq_cell_active">
    <view class="gwq_cell_hd">
      <image src="/imgs/icon_logout.png" class="icon"></image>
    </view>
    <view class="gwq_cell_bd" bindtap="onLogout">退出</view>
     <view class="gwq_cell_ft"> > </view>
  </view>
</view>
</view>