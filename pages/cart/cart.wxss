/* pages/cart/cart.wxss */
page {
  padding-bottom: 20px;
  background-color: #E6E6EA;
}
/* .content {
  padding-bottom: 20px;
  background-color: #E6E6EA;
} */
/* 查询模块 */
.gwq_search_input{
  border: 1rpx solid #E6E6EA;
}
.gwq_search{
  width: 80%;
}
.gwq_location {
  border: 1px solid #d0d0d0;
  border-radius: 10rpx;
  width: 30%;
  height: 30px;
  line-height: 180%;
  text-align: center;
  margin-left: 20rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-line-clamp:1;
  -webkit-box-orient:vertical;
}
.gwq_search_arr {
  border: 1px solid #d0d0d0;
  border-radius: 10rpx;
  margin-left: 15rpx;
}
.gwq_search_arr input{
  margin-left: 60rpx;
  height: 60rpx;
  border-radius: 5px;
}
.gwq_sousuo {
  margin-left: 15rpx;
  margin-right: 12rpx;
  width: 15%;
  line-height: 180%;
  text-align: center;
  border: 1px solid #d0d0d0;
  border-radius: 10rpx;
}
.gwq_page_row{
  display: flex;
  flex-direction: row;
  margin-top : 10px;
}
.gwq_searchcion {
  margin: 10rpx 10rpx 10rpx 200rpx;
  position: absolute;
  left:25rpx;
  z-index: 2;
  width: 20px;
  height: 20px;
  text-align: center;
}
/* 小模块 */
.gwq_model_cart {
  margin: 5px;
  width: 100%;
  height : 110px;
  margin-top : 15px;
}
.gwq_model_one {
  float: left;
  margin: 2%;
  width: 20%;
  height : 80px;
}
.gwq_model_one .onePic {
  width: 100%;
  height : 60%;
  border-radius: 50px;
}
.gwq_model_one .oneTxt {
  font-size: 18px;
  margin: auto;
}
/* 合作公司模块 */
.gwq_company_nav {
  height: 60px;
  width: 100%;
  display: block;
  margin-left : 10px;
}
.gwq_select_txt {
  margin: 2px 12px;
}
.gwq_more_select {
  float: left;
  font-size: 14px;
  margin-top : 5px;
}
.gwq_more_select label {
  margin: 1px 10px;
}
.gwq_add_company {
  width : 100%;
  margin-top: 10px;
  margin-left: 2.5%; 
}
.gwq_add_one {
  background-color: #FFFFFF;
  width: 95%;
  display: block;
  margin: 8px 0px;
  height: 85px;
}
.gwq_add_one .pic {
  float: left;
}
.gwq_add_one .title {
  margin-left: 5px;
  float: left;
  width: 60%;
  font-size: 20px;
}
.gwq_add_one .addition {
  margin-left: 5px;
  float: left;
  font-size: 13px;
  width: 70%;
  margin-top : 6px;
}
.gwq_add_one .opentime {
  margin-left: 5px;
  float: left;
  font-size: 12px;
  width: 30%;
  margin-top : 12px;
}
.gwq_add_one .distance {
  float: right;
  font-size: 12px;
  margin-top: 12px;
}