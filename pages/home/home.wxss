page {
  padding-bottom: 20px;
  background-color: #E6E6EA;
}
/* 查询模块 */
.gwq_search_input{
  border: 1rpx solid #E6E6EA;
  /* background-color: #FFFFFF; */
}
.gwq_search{
  width: 80%;
}
.gwq_search_arr {
  border: 1px solid #d0d0d0;
  border-radius: 10rpx;
  margin-left: 20rpx;
}
.gwq_search_arr input{
  margin-left: 60rpx;
  height: 60rpx;
  border-radius: 5px;
}
.gwq_sousuo {
  margin-left: 15rpx;
  width: 15%;
  line-height: 150%;
  text-align: center;
  border: 1px solid #d0d0d0;
  border-radius: 10rpx;
}
.gwq_page_row{
  display: flex;
  flex-direction: row
}
.gwq_searchcion {
  margin: 10rpx 10rpx 10rpx 10rpx;
  position: absolute;
  left:25rpx;
  z-index: 2;
  width: 20px;
  height: 20px;
  text-align: center;
}
/* 汽车模块 */
/* .gwq_hot_car_text {
  float: left;
} */
.gwq_hot_car {
  margin-top : 3px;
}
.gwq_hot_car_pic {
  background-color: #E6E6EA;
  margin: 5px;
  float: left;
  width: 47%;
  height: 150px;
}
.gwq_hot_car_pic .pic {
  width: 100%;
  height : 150px;
  background-color: #eeeeee;
}
.gwq_other_service_text {
  margin-bottom: 5px;
}
/* .car_other__service_icons {
  margin: auto;
} */
.gwq_other_service_icon {
  float: left;
  width: 50px;
  height: 80px;
  margin: 5px 20px;
}
.gwq_other_service_icon .iconPic{
  width: 100%;
  height: 60%;
  border-radius: 100px;
}
.gwq_other_service_icon .iconTxt{
  font-size: 12px;
  margin: auto;
}