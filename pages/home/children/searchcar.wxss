page {
  padding-bottom: 20px;
  background-color: #E6E6EA;
}
/* 搜索模块 */
.gwq_search_input{
  border: 1rpx solid #E6E6EA;
  /* background-color: #FFFFFF; */
}
.gwq_search{
  width: 80%;
}
.gwq_search_arr {
  border: 1px solid #d0d0d0;
  border-radius: 10rpx;
  margin-left: 20rpx;
}
.gwq_search_arr input{
  margin-left: 60rpx;
  height: 60rpx;
  border-radius: 5px;
}
.gwq_sousuo {
  margin-left: 15rpx;
  width: 15%;
  line-height: 150%;
  text-align: center;
  border: 1px solid #d0d0d0;
  border-radius: 10rpx;
}
